{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "build-next", "test"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build-next": {
      "dependsOn": ["^build-next"]
    },
    "test": {
      "dependsOn": ["^test"]
    }
  }
}
